{
  "name": "@mapbox/aws-sdk-jest",
  "version": "0.0.1",
  "description": "",
  "main": "aws-sdk.js",
  "scripts": {
    "format-js": "prettier --write '**/!(package*).{js,json}'",
    "format-md": "kramer --format",
    "format": "npm run format-js && npm run format-md",
    "lint-js": "eslint . && prettier --check '**/!(package*).{js,json}'",
    "lint-md": "kramer",
    "lint": "npm run lint-js && npm run lint-md",
    "test": "jest --coverage && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/aws-sdk-jest.git"
  },
  "author": "Mapbox, Inc.",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/mapbox/aws-sdk-jest/issues"
  },
  "homepage": "https://github.com/mapbox/aws-sdk-jest#readme",
  "devDependencies": {
    "@mapbox/eslint-config-mapbox": "^3.0.0",
    "@mapbox/kramer": "^1.0.0",
    "aws-sdk": "^2.720.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-circus": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "traverse": "^0.6.6"
  },
  "eslintConfig": {
    "extends": [
      "@mapbox/eslint-config-mapbox",
      "prettier"
    ],
    "overrides": [
      {
        "files": [
          "aws-sdk.js",
          "test/*.test.js"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write"
    ],
    "!(package*).json": [
      "prettier --write"
    ],
    "*.md": [
      "kramer",
      "kramer --format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "jest": {
    "testEnvironment": "node",
    "testRunner": "jest-circus/runner",
    "clearMocks": true,
    "transform": {},
    "coverageReporters": [
      "text",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ]
  }
}
